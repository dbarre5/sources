{

    left
    {
        type            fixedValue;
        value           uniform 0;
    }
    right
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 0;
    }
    top
    {
        type            fixedValue;
        value           uniform 0;
    }

    front
    {
        type            fixedValue;
        value           uniform 0;
    }

    back
    {
        type            fixedValue;
        value           uniform 0;
    }
  
}

