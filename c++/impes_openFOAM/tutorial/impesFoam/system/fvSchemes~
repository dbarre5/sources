FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
    ddt(Sw)	    Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(P)         Gauss linear;
}

divSchemes
{
    default         none;
}

laplacianSchemes
{
    default         Gauss linear corrected;
    laplacian(Kf,P) Gauss linear corrected;
    laplacian(kro,P) Gauss linear corrected;
    laplacian(krw,Pc) Gauss linear corrected;
    laplacian(krw,Pw) Gauss linear corrected;
    laplacian(krw,P) Gauss linear corrected;
}

interpolationSchemes
{
    default         	upwind phiw;
    interpolate(kro) 	upwind phio;
    interpolate(krw) 	upwind phiw;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
